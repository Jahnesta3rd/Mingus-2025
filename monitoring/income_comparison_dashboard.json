{
  "dashboard": {
    "id": null,
    "title": "Income Comparison Calculator - Real-time Metrics",
    "tags": ["mingus", "income-comparison", "lead-generation"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Lead Conversion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mingus_lead_capture_total[5m]) / rate(mingus_lead_capture_attempts_total[5m]) * 100",
            "legendFormat": "Conversion Rate (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 15},
                {"color": "green", "value": 25}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Salary Predictions Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mingus_salary_predictions_total[5m])",
            "legendFormat": "Predictions/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mingus_api_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(mingus_api_response_time_seconds_bucket[5m]))",
            "legendFormat": "Median"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "External API Health",
        "type": "stat",
        "targets": [
          {
            "expr": "mingus_external_api_health{api=\"bls\"}",
            "legendFormat": "BLS API"
          },
          {
            "expr": "mingus_external_api_health{api=\"census\"}",
            "legendFormat": "Census API"
          },
          {
            "expr": "mingus_external_api_health{api=\"fred\"}",
            "legendFormat": "FRED API"
          },
          {
            "expr": "mingus_external_api_health{api=\"bea\"}",
            "legendFormat": "BEA API"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Down", "color": "red"},
                  "1": {"text": "Up", "color": "green"}
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Lead Engagement Scores",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(mingus_lead_engagement_score_bucket[5m])) by (le)",
            "legendFormat": "{{le}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "scheme"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mingus_database_connections_active[5m])",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(mingus_database_query_duration_seconds_sum[5m]) / rate(mingus_database_query_duration_seconds_count[5m])",
            "legendFormat": "Avg Query Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Redis Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "mingus_redis_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "rate(mingus_redis_cache_hits_total[5m]) / (rate(mingus_redis_cache_hits_total[5m]) + rate(mingus_redis_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Celery Task Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mingus_celery_tasks_completed_total[5m])",
            "legendFormat": "Tasks Completed/min"
          },
          {
            "expr": "rate(mingus_celery_tasks_failed_total[5m])",
            "legendFormat": "Tasks Failed/min"
          },
          {
            "expr": "mingus_celery_workers_active",
            "legendFormat": "Active Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mingus_errors_total[5m])",
            "legendFormat": "Errors/min"
          },
          {
            "expr": "rate(mingus_errors_total[5m]) / rate(mingus_requests_total[5m]) * 100",
            "legendFormat": "Error Rate (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "Cultural Content Generation",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mingus_cultural_content_generated_total[5m])",
            "legendFormat": "Content Generated/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 11,
        "title": "Gamification Badges Unlocked",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mingus_badges_unlocked_total[5m])",
            "legendFormat": "Badges/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 12,
        "title": "Email Sequence Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mingus_emails_sent_total[5m])",
            "legendFormat": "Emails Sent/min"
          },
          {
            "expr": "rate(mingus_emails_opened_total[5m]) / rate(mingus_emails_sent_total[5m]) * 100",
            "legendFormat": "Open Rate (%)"
          },
          {
            "expr": "rate(mingus_emails_clicked_total[5m]) / rate(mingus_emails_sent_total[5m]) * 100",
            "legendFormat": "Click Rate (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "location",
          "type": "query",
          "query": "label_values(mingus_lead_capture_total, location)",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "industry",
          "type": "query",
          "query": "label_values(mingus_lead_capture_total, industry)",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(mingus_deployment_version[1m]) > 0",
          "iconColor": "blue",
          "titleFormat": "Deployment: {{ $value }}"
        },
        {
          "name": "Errors",
          "datasource": "Prometheus",
          "expr": "rate(mingus_errors_total[5m]) > 0.1",
          "iconColor": "red",
          "titleFormat": "High Error Rate: {{ $value }} errors/min"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "API Documentation",
        "url": "/api/docs/",
        "targetBlank": true
      },
      {
        "title": "Admin Panel",
        "url": "/admin/",
        "targetBlank": true
      }
    ]
  }
} 